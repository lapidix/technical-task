{
  "name": "technical-task",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "source .env.deploy && npm run build && npx open-next@latest build && serverless deploy",
    "deploy:prod": "source .env.deploy && npm run build && npx open-next@latest build && serverless deploy --stage prod",
    "deploy:full": "source .env.deploy && npm run deploy && aws s3 sync .open-next/assets s3://nextjs-lambda-app-assets-dev --delete",
    "sync:s3": "aws s3 sync .open-next/assets s3://nextjs-lambda-app-assets-dev --delete"
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^2.2.0",
    "@tanstack/react-query": "^5.90.6",
    "clsx": "^2.1.1",
    "next": "15.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "viem": "^2.38.6",
    "wagmi": "^2.19.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "open-next": "^3.1.3",
    "postcss": "^8.5.6",
    "serverless": "^4.23.0",
    "serverless-domain-manager": "^8.0.0",
    "tailwindcss": "^3.4.18",
    "typescript": "^5"
  }
}
